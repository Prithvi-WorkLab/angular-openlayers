{
  "name": "angular-openlayers",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --port=4201",
    "build": "ng build --prod",
    "build:local": "ng build",
    "test": "ng test --code-coverage  --progress=true",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "doc:local": "compodoc -p projects/tsconfig.compodoc.json -s -o -w",
    "doc:project": "lerna exec --parallel -- compodoc -p ./tsconfig.lib.json --hideGenerator --disableCoverage --output='../../documentation/${LERNA_PACKAGE_NAME}/'",
    "doc:app": "compodoc -p src/tsconfig.app.json --hideGenerator --disableCoverage --output='documentation/'",
    "docs": "npm run doc:app && npm run doc:project"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/http": "~7.2.15",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/pwa": "^0.803.21",
    "@angular/router": "~8.2.14",
    "@angular/service-worker": "~8.2.14",
    "@angular/youtube-player": "^8.2.3",
    "@clr/angular": "^2.4.4",
    "@webcomponents/custom-elements": "^1.3.1",
    "copyfiles": "^2.1.1",
    "core-js": "^3.4.1",
    "hammerjs": "^2.0.8",
    "ol": "^5.3.0",
    "puppeteer": "^2.0.0",
    "puppeteer-core": "^2.0.0",
    "rimraf": "^3.0.0",
    "rxjs": "~6.5.3",
    "serve": "^11.2.0",
    "tslib": "^1.10.0",
    "youtube-player": "^5.5.2",
    "zone.js": "~0.10.2",
    "@clr/ui": "2.4.4",
    "@clr/icons": "2.4.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.29",
    "@angular-devkit/build-ng-packagr": "~0.803.29",
    "@angular/cli": "~8.3.29",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "~3.4.6",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~12.12.7",
    "@types/ol": "^5.3.6",
    "codelyzer": "~5.2.0",
