    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "lerna": "^3.19.0",
    "ng-packagr": "^14.0.2",
    "protractor": "~7.0.0",
    "ts-node": "~10.8.2",
    "tslint": "~6.1.3",
    "typescript": "~4.6.4"
