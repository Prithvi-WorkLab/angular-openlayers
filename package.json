{
  "name": "angular-openlayers",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --port=4201",
    "build": "ng build --configuration production",
    "build:local": "ng build",
    "test": "ng test --code-coverage  --progress=true",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "doc:local": "compodoc -p projects/tsconfig.compodoc.json -s -o -w",
    "doc:project": "lerna exec --parallel -- compodoc -p ./tsconfig.lib.json --hideGenerator --disableCoverage --output='../../documentation/${LERNA_PACKAGE_NAME}/'",
    "doc:app": "compodoc -p src/tsconfig.app.json --hideGenerator --disableCoverage --output='documentation/'",
    "docs": "npm run doc:app && npm run doc:project"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.3.11",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "~13.3.11",
    "@angular/compiler": "~13.3.11",
    "@angular/core": "~13.3.11",
    "@angular/forms": "~13.3.11",
    "@angular/http": "~7.2.15",
    "@angular/material": "^13.3.9",
    "@angular/platform-browser": "~13.3.11",
    "@angular/platform-browser-dynamic": "~13.3.11",
    "@angular/router": "~13.3.11",
    "@angular/service-worker": "~13.3.11",
    "@angular/youtube-player": "^11.2.13",
    "@clr/angular": "^2.4.4",
    "@clr/icons": "2.4.4",
    "@clr/ui": "2.4.4",
    "@webcomponents/custom-elements": "^1.3.1",
    "copyfiles": "^2.1.1",
    "core-js": "^3.4.1",
    "ol": "^5.3.0",
    "puppeteer": "^2.0.0",
    "puppeteer-core": "^2.0.0",
    "rimraf": "^3.0.0",
    "rxjs": "~6.5.3",
    "serve": "^11.2.0",
    "tslib": "^2.0.0",
    "youtube-player": "^5.5.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.8",
    "@angular-devkit/build-ng-packagr": "~0.1002.4",
    "@angular/cli": "~13.3.8",
    "@angular/compiler-cli": "~13.3.11",
    "@angular/language-service": "~13.3.11",
    "@compodoc/compodoc": "^1.1.11",
    "@types/jasmine": "~3.4.6",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.11.1",
    "@types/ol": "^5.3.6",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lerna": "^3.19.0",
    "ng-packagr": "^13.3.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.5.2",
    "tslint": "~6.1.0",
    "typescript": "~4.6.4"
  }
}