    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^6.3.14",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "lerna": "^3.19.0",
    "ng-packagr": "^5.7.1",
    "protractor": "~5.4.2",
    "ts-node": "~8.5.2",
    "tsickle": ">=0.37.1",
    "tslib": "^1.10.0",
    "tslint": "~5.20.1",
